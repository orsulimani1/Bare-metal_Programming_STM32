#include <stdint.h>


int main(void)
{
	int value = 42;
	int *ptr = &value;
	printf("value %d ,&value %x, ptr %x\n", value, &value,  ptr);
	*ptr = 100;    // Changes value to 100 through the pointer
	printf("value %d ,&value %x, ptr %x\n", value, &value,  ptr); // Prints 100
	for(;;);
}

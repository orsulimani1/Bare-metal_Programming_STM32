#include <stdint.h>

/* Retarget the C library printf function to the SWO */
#ifdef __GNUC__
// For GCC-based toolchains:
int _write(int file, char *ptr, int len) {
    for (int i = 0; i < len; i++) {
        ITM_SendChar(ptr[i]);   // ITM_SendChar() is provided by CMSIS
    }
    return len;
}
#else
// For other compilers (e.g., ARMCC), override fputc or fputchar
int fputc(int ch, FILE *f) {
    ITM_SendChar(ch);
    return ch;
}
#endif
